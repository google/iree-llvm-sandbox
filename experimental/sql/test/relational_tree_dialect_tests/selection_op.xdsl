// RUN: rel_opt.py %s | filecheck %s

module() {
    rel_tree.selection() {
        rel_tree.pandas_table() ["table_name" = "t"] {
            rel_tree.schema_element() ["elt_name" = "id", "elt_type" = !rel_tree.int32]
        }
    } {
        rel_tree.predicate() ["comparator" = "="] {
            rel_tree.column() ["col_name" = "a"]
        } {
            rel_tree.literal() ["val" = 5 : !i64, "type" = !rel_tree.int32]
        }
    }
}

CHECK: rel_tree.selection() {
CHECK-NEXT:         rel_tree.pandas_table() ["table_name" = "t"] {
CHECK-NEXT:             rel_tree.schema_element() ["elt_name" = "id", "elt_type" = !rel_tree.int32]
CHECK-NEXT:         }
CHECK-NEXT:     } {
CHECK-NEXT:         rel_tree.predicate() ["comparator" = "="] {
CHECK-NEXT:             rel_tree.column() ["col_name" = "a"]
CHECK-NEXT:         } {
CHECK-NEXT:             rel_tree.literal() ["val" = 5 : !i64, "type" = !rel_tree.int32]
CHECK-NEXT:         }
CHECK-NEXT:     }
