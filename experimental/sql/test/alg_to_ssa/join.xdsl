// RUN: rel_opt.py -p alg-to-ssa %s | filecheck %s

module() {
  rel_alg.inner_join() {
    rel_alg.table() ["table_name" = "t"] {
      rel_alg.schema_element() ["elt_name" = "a", "elt_type" = !rel_alg.int64]
      rel_alg.schema_element() ["elt_name" = "b", "elt_type" = !rel_alg.int64]
    }
  } {
    rel_alg.table() ["table_name" = "u"] {
      rel_alg.schema_element() ["elt_name" = "c", "elt_type" = !rel_alg.int64]
      rel_alg.schema_element() ["elt_name" = "d", "elt_type" = !rel_alg.int64]
    }
  } {
    rel_alg.compare() ["comparator" = "="] {
      rel_alg.column() ["col_name" = "a"]
    } {
      rel_alg.column() ["col_name" = "c"]
    }
  }
}

//      CHECK:  %{{.*}} : !rel_ssa.bag<[!rel_ssa.schema_element<"a", !rel_ssa.int64>, !rel_ssa.schema_element<"b", !rel_ssa.int64>]> = rel_ssa.table() ["table_name" = "t"]
// CHECK-NEXT:  %{{.*}} : !rel_ssa.bag<[!rel_ssa.schema_element<"c", !rel_ssa.int64>, !rel_ssa.schema_element<"d", !rel_ssa.int64>]> = rel_ssa.table() ["table_name" = "u"]
// CHECK-NEXT:  %{{.*}} : !rel_ssa.bag<[!rel_ssa.schema_element<"a", !rel_ssa.int64>, !rel_ssa.schema_element<"b", !rel_ssa.int64>, !rel_ssa.schema_element<"c", !rel_ssa.int64>, !rel_ssa.schema_element<"d", !rel_ssa.int64>]> = rel_ssa.inner_join(%{{.*}} : !rel_ssa.bag<[!rel_ssa.schema_element<"a", !rel_ssa.int64>, !rel_ssa.schema_element<"b", !rel_ssa.int64>]>, %{{.*}} : !rel_ssa.bag<[!rel_ssa.schema_element<"c", !rel_ssa.int64>, !rel_ssa.schema_element<"d", !rel_ssa.int64>]>) {
// CHECK-NEXT:    %{{.*}} : !rel_ssa.int64 = rel_ssa.column() ["col_name" = "a"]
// CHECK-NEXT:    %{{.*}} : !rel_ssa.int64 = rel_ssa.column() ["col_name" = "c"]
// CHECK-NEXT:    %{{.*}} : !rel_ssa.bool = rel_ssa.compare(%{{.*}} : !rel_ssa.int64, %{{.*}} : !rel_ssa.int64) ["comparator" = "="]
// CHECK-NEXT:    rel_ssa.yield_value(%{{.*}} : !rel_ssa.bool)
// CHECK-NEXT:  }
