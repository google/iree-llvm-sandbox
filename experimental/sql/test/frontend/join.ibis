# RUN: rel_opt.py -f ibis %s | filecheck %s

t = ibis.table([("id", "int64"), ("a", "int64")], 't')
u = ibis.table([("id", "int64"), ("b", "int64")], 'u')
t.join(u, t.id == u.id)

